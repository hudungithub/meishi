  margin-top:10rpx;
}
/*头部*/
.toubu{
  height: 300rpx;
  background-color: #FFA114;
  border-radius:0 0 100rpx 100rpx;  
}
.touxiang{
  position: absolute;
  top: 30rpx;
  left: 41%;
