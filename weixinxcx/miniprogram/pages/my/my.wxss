}
/*订单*/
.dingdan{
  width: 660rpx;height: 220rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: -80rpx;
  border-radius: 10rpx;
}
.dingdan1{
  width: 620rpx;height: 90rpx;
  border-bottom: 1px solid rgb(239, 239, 239);
  margin: 0 auto;
  line-height: 90rpx;
}
.dingdanz{
  font-size: 40rpx;
  padding-left: 40rpx;
}
.dingdanx{
  float: right;
  font-size: 60rpx;
  color: #999;
}
.dingdan2 view{
  width: 100rpx;height: 100rpx;
  margin-left: 50rpx;
  margin-top: 10rpx;
  float: left;
}
.dingdan2 view image{
  width: 80rpx;height: 80rpx;
}
.dingdan2 view text{
  display: block;
  font-size: 25rpx;
  margin-top: -10rpx;
}
/*信息*/
.xinxi{
  width: 660rpx;height: 736rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 10rpx;
}
.xinxi view{
  width: 620rpx;height: 100rpx;
  border-bottom: 1px solid rgb(239, 239, 239);
  margin: 0 auto;
}
.xinxi view image{
  width: 60rpx;height: 60rpx;
  margin-left: 50rpx;
  margin-top: 20rpx;
}
.xinxi view text{
  position: absolute;
  margin-left:40rpx;
